%matplotlib inline
# Imports
import pandas as pd
import matplotlib as plt

# Constant Definition
DATASET_NAME = "/CEOWages.csv"
X = ['educ','exper', 'tenure']
Y = 'wage'

# Subfunctions
def readCSVAsDataFrame(psDataSetName):
    SEPERATOR = ","
    ERROR_MSG = "File Not Found Exception Occured"
    try :
         return pd.read_csv(DATASET_NAME,SEPERATOR)   
    except :
        print(ERROR_MSG)

# Implementation        
df = readCSVAsDataFrame(DATASET_NAME)
df.boxplot(X[1])

